    <validationRules>
        <fullName>CheckRequestDates</fullName>
        <active>true</active>
        <description>Check that selected latest request date is &gt;=  the earliest request date</description>
        <errorConditionFormula>Earliest_Date_of_Visit__c  &gt; Latest_Date_of_Visit__c</errorConditionFormula>
        <errorMessage>Earliest Date of Visit must be before or equal to Latest Date of Visit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chimney_Rock_No_Weekend_Reservations</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

(Winery_Name__c = &quot;Chimney Rock&quot;),
OR( (Earliest_Date_of_Visit_Day_of_Week__c = &quot;Saturday&quot;),
(Earliest_Date_of_Visit_Day_of_Week__c = &quot;Sunday&quot;) )

)

&amp;&amp; not($Permission.Chimney_Rock_Weekend_Override)</errorConditionFormula>
        <errorMessage>Please ensure all Chimney Rock Reservations are during the work week</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirmed_DateTime_Before_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  OR( ISBLANK(TEXT(Confirmed_Date_of_Visit__c)),
      ISBLANK(TEXT(Confirmed_Time_of_Visit__c ))),
  ISPICKVAL( Approval_Status__c, &quot;Approved&quot;))</errorConditionFormula>
        <errorMessage>Please populate Confirmed Date of Visit and Confirmed Time of Visit before Approving</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequestLeadTime</fullName>
        <active>false</active>
        <description>Checks the a tour and tasting earliest request is 7 days from the date of request</description>
        <errorConditionFormula>(Earliest_Date_of_Visit__c ) &lt;=  DATEVALUE(now()) + 7

&amp;&amp; not($Permission.Lead_Time_Override)</errorConditionFormula>
        <errorMessage>Tour and Tasting requests must be made 7 days in advance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequestType_must_be_filled</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(Request_Type_text__c)</errorConditionFormula>
        <errorMessage>Please populate Request Type field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rochioli_Winery_Start_Time</fullName>
        <active>false</active>
        <errorConditionFormula>AND((Winery_Name__c = &quot;Chimney Rock&quot;),(Earliest_Date_of_Visit__c == DATE(2019,12, 25)))</errorConditionFormula>
        <errorMessage>Closed on Christmas</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rutherford_Hill_Winery_Available_Mon_Fri</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND(
(Winery_Name__c = &quot;Rutherford Hill Winery&quot;),
OR( (Earliest_Date_of_Visit_Day_of_Week__c= &quot;Saturday&quot;),
(Earliest_Date_of_Visit_Day_of_Week__c= &quot;Sunday&quot;) )
), AND(
(Winery_Name__c = &quot;Rutherford Hill Winery&quot;),
OR( (Earliest_Date_of_Visit_Day_of_Week__c!= &quot;Saturday&quot;),
(Earliest_Date_of_Visit_Day_of_Week__c!= &quot;Sunday&quot;) ), ISPICKVAL(Latest_Start_Time__c, &apos;4:00 PM PDT&apos;)
))</errorConditionFormula>
        <errorMessage>Private Tours &amp; Tasting are available M-F (last appointment at 3:30pm)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateConfirmedTastingDate</fullName>
        <active>true</active>
        <description>Validates that the tasting is confirmed within the date range.</description>
        <errorConditionFormula>Earliest_Date_of_Visit__c &gt; Confirmed_Date_of_Visit__c 
|| 
Confirmed_Date_of_Visit__c &gt; Latest_Date_of_Visit__c</errorConditionFormula>
        <errorMessage>Confirmed Tasting date cannot be outside of requested date range.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Winery_No_Food_Available</fullName>
        <active>true</active>
        <description>Based on checkbox on Account</description>
        <errorConditionFormula>AND( 

( Lunch_Request__c = TRUE), 

( Winery__r.Food_Available__c = FALSE) 
)</errorConditionFormula>
        <errorMessage>This Winery cannot accommodate Lunch Requests</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Winery_No_Lodging</fullName>
        <active>true</active>
        <description>Based on checkbox on Account</description>
        <errorConditionFormula>AND( 

(Lodging_Request__c = TRUE), 

(Winery__r.Lodging_Available__c = FALSE) 
)</errorConditionFormula>
        <errorMessage>This Winery cannot accommodate Lodging Requests</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Winery_No_Private_Tour</fullName>
        <active>true</active>
        <description>Based on checkbox on Account</description>
        <errorConditionFormula>AND( 


ISPICKVAL(Request_Type__c, &quot;Tour&quot;), 
(Winery__r.Private_Tours__c = FALSE) 

)</errorConditionFormula>
        <errorMessage>This Winery cannot accommodate Private Tours</errorMessage>
    </validationRules>
