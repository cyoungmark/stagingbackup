    <validationRules>
        <fullName>Cannot_have_Account_Record_Type_Supplier</fullName>
        <active>false</active>
        <description>Validation rule to stop user from proceeding with a T&amp;T form if the associated Account record type is &quot;Supplier&quot;.</description>
        <errorConditionFormula>TEXT (Primary_Guest_Name__r.Account.Type)  = &apos;Supplier&apos;</errorConditionFormula>
        <errorMessage>You cannot create a Tour &amp; Tasting Form with a Supplier account.  Please select a different account that is not a Supplier.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DeleteAttachmentNotAllowed</fullName>
        <active>false</active>
        <errorConditionFormula>AND ( 

NOT(IsNew()), 

RecordType.Name = &quot;Trade Account&quot; , 
Primary_Guest_Name__r.Account.New_Account__c = false, 
Number_of_Attachments__c = 0,
 ISCHANGED(Number_of_Attachments__c) 
)</errorConditionFormula>
        <errorMessage>Cannot Delete Diver Report</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_to_Add_Request</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 

    OR( 
       (ISPICKVAL( Tour_Form_Approval_Status__c ,&quot;Submit for Approval&quot;)),
       (ISPICKVAL(Tour_Form_Approval_Status__c ,&quot;Approved&quot;))),

Number_of_Requests__c &lt; 1 

)</errorConditionFormula>
        <errorMessage>Please add at least one winery Tour and Tasting Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Contact_Mobile_Required</fullName>
        <active>true</active>
        <description>Rule to ensure that the listed Primary Guest has a mobile phone number listed on the corresponding Contact record.</description>
        <errorConditionFormula>ISNULL(Primary_Guest_Name__r.MobilePhone)</errorConditionFormula>
        <errorMessage>The Primary Guest must have a mobile phone number on the Contact record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Attachment_To_Continue</fullName>
        <active>false</active>
        <description>DISABLED BY MIKE TOPALOVICH PER CARLA 4/17

Required only for Retailer Type of Tour</description>
        <errorConditionFormula>AND( 
ISPICKVAL( Request_Type__c,&quot;Retailer&quot;),	
OR( 
ISPICKVAL( Tour_Form_Approval_Status__c ,&quot;Approved&quot;), 
ISPICKVAL( Tour_Form_Approval_Status__c ,&quot;Submit for Approval&quot;)), 

Number_of_Attachments__c = 0 

)</errorConditionFormula>
        <errorMessage>Please attach a Diver Report for Retailer Requests</errorMessage>
    </validationRules>
